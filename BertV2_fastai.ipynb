    "# Defining a function for calculating accuracy\n",
    "def logits_accuracy(logits, labels):\n",
    "    probs = torch.sigmoid(logits.unsqueeze(-1))\n",
    "    preds = (probs > 0.5).long()\n",
    "    acc = (preds.squeeze() == labels).float().mean()\n",
    "    return acc"
   ]
