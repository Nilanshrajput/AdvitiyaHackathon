    "  tokens_ids_tensor = torch.tensor(tokens_ids)\n",
    "\n",
    "  # Obtaining the attention mask\n",
    "  attn_mask = (tokens_ids_tensor != 0).long()\n",
    "\n",
    "  return tokens_ids_tensor, attn_mask\n"
